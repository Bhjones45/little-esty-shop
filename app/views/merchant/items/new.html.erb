<h1>Create New Item</h1>
<%= form_with url: "/merchants/#{params[:merchant_id]}/items", method: :post, local: true do |f| %>
  <p>
  <%= f.label :name %>
  <%= f.text_field :name %>
  </p>
  <p>
  <%= f.label :description %>
  <%= f.text_field :description %>
  </p>
  <p>
  <%= f.label :unit_price, "Price $" %>
  <%= f.number_field :unit_price %>
  <%= f.hidden_field :enabled, value: "disabled" %>
  </p>
  <%= f.submit "Submit" %>
<% end %>
